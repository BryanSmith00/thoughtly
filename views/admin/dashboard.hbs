<div class="dash-content">
    {{#if users}}
        <div class="feed-container" id="feed">
            {{#each users}}
            <div class="thought">
                
                <p>{{displayName}}</p>

                <a href="/admin/edit/{{_id}}" class="btn"> <i class=""></i> </a>  
            </div>
            {{/each}}
        </div>

    {{else}}
        <p>There are no users</p>
    {{/if}}
</div>